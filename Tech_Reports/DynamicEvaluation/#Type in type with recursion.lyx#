\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{x:A\in H}{\Gamma\sim H\vdash x\,Elab\,x::A}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\sim H\vdash M\,Elab_{\star,l}\,C\quad\Gamma\sim H\vdash m\,Elab_{C,l}\,a}{\Gamma\sim H\vdash m::_{l}M\,Elab\,a}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{???}{\Gamma\sim H\vdash\star\,Elab\,\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\sim H\vdash M\,Elab_{\star,l}\,A\quad\Gamma,x:M\sim H,x:A\vdash N\,Elab_{\star,l'}\,B}{\Gamma\vdash\Pi x:M_{l}.N_{l'}\,Elab\,\left(\Pi x:A.B\right)::\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\sim H\vdash m\,Elab\,b::e\quad\left(\Pi x:A.B\right)=e\uparrow\quad\Gamma\sim H\vdash n\,Elab_{A,l}\,a}{\Gamma\sim H\vdash m\,_{l}n\,Elab\,b\,a}
\]

\end_inset


\end_layout

\begin_layout Subsection
Check
\end_layout

\begin_layout Standard
TODO probably easiest to extend the elaboration checking judgment with the
 raw terms of the base lang, so everything can move in lock step
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{???}{\Gamma\sim H\vdash\star\,Elab_{\star,l}\,\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
