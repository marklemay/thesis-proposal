if 
\begin_inset Formula $e'\downarrow(x:M')\rightarrow N'$
\end_inset

 then 
\begin_inset Formula $e\downarrow(x:M)\rightarrow N$
\end_inset

 
\end_layout

\begin_layout Enumerate
if 
\begin_inset Formula $e'\downarrow TCon\,\overline{M'}$
\end_inset

 then 
\begin_inset Formula $e\downarrow TCon\,\overline{M}$
\end_inset

 
\end_layout

\end_deeper
