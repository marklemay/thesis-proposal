\begin_layout Subsection
Elaboration
\end_layout

\begin_layout Subsubsection
Infer
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{x:A\in H}{H\vdash x\,Elab\,x::A}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash M\,Elab_{\star,l}\,C\quad H\vdash m\,Elab_{C,l}\,a}{H\vdash m::_{l}M\,Elab\,a}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash}{H\vdash\star\,Elab\,\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash M\,Elab_{\star,l}\,A\quad H,x:A\vdash N\,Elab_{\star,l'}\,B}{H\vdash\Pi x:M_{l}.N_{l'}\,Elab\,\Pi x:A.B}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash m\,Elab\,b_{h}::e\quad\Pi x:A.B=e\uparrow\quad H\vdash n\,Elab_{A,l}\,a}{H\vdash m\,_{l}n\,Elab\,\left(b_{h}::e\right)\,a}
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Check
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash}{H\vdash\star\,Elab_{\star,l}\,\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H,f:\Pi x:A.B,\,x:A\vdash m\,Elab_{B,l}\,b}{H\vdash\mathsf{fun}\,f.\,x.\,m\,Elab_{\Pi x:A.B,l}\,\mathsf{fun}\,f.\,x.b}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash m\,Elab\,a_{h}::e}{H\vdash m\,Elab_{A,l}\,a_{h}::e=_{l,.}A}
\]

\end_inset


\end_layout

\begin_layout Subsection
Typing
\end_layout

\begin_layout Subsubsection
Term Typing
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash}{H\vdash\star:\star}\star-ty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash A:\star\quad H,x:A\vdash B:\star}{H\vdash\Pi x:A.B\,:\star}\Pi-ty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash a:A\quad H\vdash A\equiv A':\star}{H\vdash a:A'}conv
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash e:\overline{\star}\quad H\vdash a_{h}:B\downarrow}{H\vdash a_{h}::e\quad:\quad e\uparrow}apparent
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Head Typing
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{x:A\in H}{H\vdash x:A}var-ty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H,f:\Pi x:A.B,x:A\vdash b:B}{H\vdash\mathsf{fun}\,f.\,x.b\,:\,\Pi x:A.B}\Pi-\mathsf{fun}-ty
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash b:\Pi x:A.B\quad H\vdash a:A}{H\vdash b\,a\,:\,B\left[x\coloneqq a\right]}\Pi-app-ty
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Cast Typing
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash A:\star}{H\vdash A:\overline{\star}}eq-ty-1
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash e:\overline{\star}\quad H\vdash A:\star}{H\vdash e=_{l,o}A:\overline{\star}}eq-ty-2
\]

\end_inset


\end_layout

\begin_layout Subsection
Definitional Equality
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash a\Rrightarrow_{*}b:A\quad H\vdash a'\Rrightarrow_{*}b'\,:A\quad H\vdash b\sim b':A}{H\vdash a\equiv a':A}
\]

\end_inset


\end_layout

\begin_layout Subsection
Consistent
\end_layout

\begin_layout Standard
A relation that equates terms except for source location and observation
 information
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\star\sim\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{A\sim A'\quad B\sim B'}{\Pi x:A.B\sim\Pi x:A'.B'}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{a_{h}\sim a'_{h}\quad e\sim e'}{a_{h}::e\sim a'_{h}::e'}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{e\sim e'\quad A\sim A'}{e=_{l,o}A\sim e'=_{l',o'}A'}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{a\sim a'}{\mathsf{fun}\,f.\,x.a\sim\mathsf{fun}\,f.\,x.a'}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{b\sim b'\quad a\sim a'}{b\,a\sim b'\,a'}
\]

\end_inset


\end_layout

\begin_layout Subsection
Parallel Reductions
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash a:A}{H\vdash a\Rrightarrow_{*}a:A}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{H\vdash a\Rrightarrow_{*}b:A\quad H\vdash b\Rrightarrow c:A}{H\vdash a\Rrightarrow_{*}c:A}
\]

\end_inset


\end_layout

\begin_layout Subsection
Parallel Reduction
\end_layout

\begin_layout Subsubsection
Term Par reduction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\star\Rrightarrow\star}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{A\Rrightarrow A'\quad B\Rrightarrow B'}{\Pi x:A.B\Rrightarrow\Pi x:A'.B'}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{a_{h}\Rrightarrow a'_{h}\quad e\Rrightarrow e'}{a_{h}::e\Rrightarrow a'_{h}::e'}
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Head Par reduction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{b\Rrightarrow b'\quad a\Rrightarrow a'\quad e\,Elim_{\Pi}\,x:e_{A}.e_{B}\quad e_{B}\Rrightarrow e_{B}'}{\left(\mathsf{fun}\,f.\,x.b\right)::e\,a\Rrightarrow\left(b'\left[f\coloneqq\left(\mathsf{fun}\,f.\,x.b'\right),x\coloneqq a'::e_{A}\right]::e_{B}'\left[x\coloneqq a'\right]\right)}\Pi C\Rrightarrow
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{x\Rrightarrow x}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{b\Rrightarrow b'}{\mathsf{fun}\,f.\,x.b\Rrightarrow\mathsf{fun}\,f.\,x.b'}\Pi I\Rrightarrow
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{b\Rrightarrow b'\quad a\Rrightarrow a'}{b\,a\Rrightarrow b'\,a'}\Pi E\Rrightarrow
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Cast Par reduction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{e\Rrightarrow e'\quad A\Rrightarrow A'\quad o\Rrightarrow o'}{e=_{l,o}A\Rrightarrow e'=_{l,o'}A'}
\]

\end_inset


\end_layout

\begin_layout Standard
annoyingly need to support observation reductions, to allow a substitution
 lemma to simplify the proof
\end_layout

\begin_layout Subsubsection
Observation Par reduction
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{.\Rrightarrow.}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{o\Rrightarrow o'}{o.arg\Rrightarrow o'.arg}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{o\Rrightarrow o'\quad a\Rrightarrow a'}{o.bod[a]\Rrightarrow o'.bod[a']}
\]

\end_inset


\end_layout

