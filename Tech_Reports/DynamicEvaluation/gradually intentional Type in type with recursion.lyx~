\begin_layout Section*
Judgments
\end_layout

\begin_layout Standard
The following judgments are mutually inductively defined.
\end_layout

\begin_layout Subsection*
Definition Equality
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\sigma}{\Gamma\vdash M\equiv M:\sigma}\,\textrm{Tm-Eq-R}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M\equiv N:\sigma}{\Gamma\vdash N\equiv M:\sigma}\,\textrm{Tm-Eq-S}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M\equiv N:\sigma\qquad\Gamma\vdash N\equiv P:\sigma}{\Gamma\vdash M\equiv P:\sigma}\,\textrm{Tm-Eq-T}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\sigma\qquad\Gamma\vdash\sigma\equiv\tau:\star}{\Gamma\vdash M:\tau}\,\textrm{Tm-Conv}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Context Rules
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\lozenge\vdash}\,\textrm{C-Emp}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\Gamma\vdash\sigma:\star}{\Gamma,x:\sigma\vdash}\,\textrm{C-Ext}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Variables
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:\sigma,\Delta\vdash}{\Gamma,x:\sigma,\Delta\vdash x:\sigma}\,\textrm{Var}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Type-in-Type
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash}{\Gamma\vdash\star:\star}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Dependent recursive Functions
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash\sigma:\star\qquad\Gamma,x:\sigma\vdash\tau:\star}{\Gamma\vdash\Pi x:\sigma.\tau\,:\star}\,\Pi\textrm{-F}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:\sigma\vdash\tau:\star\qquad\Gamma,x:\sigma,f:\Pi x:\sigma.\tau\vdash M:\tau}{\Gamma\vdash\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\,:\,\varPi x:\sigma.\tau}\,\Pi\textrm{-I}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\,\Pi x:\sigma.\tau\qquad\Gamma\vdash N:\sigma}{\Gamma\vdash M\,N\,:\,\tau\left[x\coloneqq N\right]}\,\Pi\textrm{-E}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:\sigma\vdash\tau:\star\qquad\Gamma,x:\sigma,f:\Pi x:\sigma.\tau\vdash M:\tau\qquad\Gamma\vdash N:\sigma}{\Gamma\vdash\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\right)\,N\,\equiv\,M\left[x\coloneqq N,f\coloneqq\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\right)\right]\,:\,\tau\left[x\coloneqq N\right]}\,\Pi\textrm{-C}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\right)\,v\,\rightsquigarrow\,M\left[x\coloneqq v,f\coloneqq\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\right)\right]}\,\Pi\textrm{\textrm{-C-Step}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{M\,\rightsquigarrow\,M'}{M\,N\,\rightsquigarrow\,M'\,N}\,\Pi\textrm{-E-Step-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{N\,\rightsquigarrow\,N'}{v\,N\,\rightsquigarrow\,v\,N'}\,\Pi\textrm{-E-Step-2}
\]

\end_inset


\end_layout

\begin_layout Standard
Standard Definition Equality rules
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash\tau\equiv\tau'\,:\star\qquad\Gamma\vdash\sigma\equiv\sigma'\,:\star}{\Gamma\vdash\Pi x:\sigma.\tau\,\equiv\,\Pi x:\sigma'.\tau'\,:\star}\,\Pi\textrm{-Eq}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:\sigma\vdash M\equiv M':\tau}{\Gamma\vdash\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M\right)\,\equiv\,\left(\mathsf{fun}\,f:\left(x.\tau\right).\,x:\sigma.M'\right):\,\varPi x:\sigma.\tau}\,\Pi\textrm{\textrm{-I}-Eq}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M\equiv\,M':\,\Pi x:\sigma.\tau\qquad\Gamma\vdash N\equiv\,N':\sigma}{\Gamma\vdash M\,N\,\equiv\,M'\,N':\,\tau\left[x\coloneqq N\right]}\,\Pi\textrm{\textrm{-E}-Eq}
\]

\end_inset


\end_layout

\begin_layout Standard
From here on the Definition Equality rules will not be presented to avoid
 clutter.
 Definition Equality distributes over all term and type syntax.
\end_layout

\begin_layout Subsection*
Propositional Equalities
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\sigma\qquad\Gamma\vdash N:\sigma}{\Gamma\vdash\mathtt{Id}_{\sigma}\left(M,N\right)\,:\star}\,\textrm{Id-F}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\sigma}{\Gamma\vdash\mathtt{Refl}_{\sigma}\left(M\right):\mathtt{Id}_{\sigma}\left(M,M\right)}\,\textrm{Id-I}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\begin{array}{c}
\Gamma\vdash\sigma:\star\\
\Gamma,x:\sigma,y:\sigma,p:\mathtt{Id}_{\sigma}\left(x,y\right)\vdash\tau:\star\\
\Gamma,z:\sigma\vdash M_{H}:\tau\left[x\coloneqq z,x\coloneqq z,p\coloneqq\mathtt{Refl}_{\sigma}\left(z\right)\right]\\
\Gamma\vdash M:\sigma\\
\Gamma\vdash N:\sigma\\
\Gamma\vdash P:\mathtt{Id}_{\sigma}\left(M,N\right)
\end{array}}{\Gamma\vdash\mathsf{Case}_{\left(p\right):\mathtt{Id}\left(x,y\right).\tau}^{\mathtt{Id}_{\sigma}}\left(z:\sigma.M_{H},M,N,P\right):\tau\left[x\coloneqq M,y\coloneqq N,p\coloneqq P\right]}\,\textrm{Id-E}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\begin{array}{c}
\Gamma\vdash\sigma:\star\\
\Gamma,x:\sigma,y:\sigma,p:\mathtt{Id}_{\sigma}\left(x,y\right)\vdash\tau:\star\\
\Gamma,z:\sigma\vdash M_{H}:\tau\left[x\coloneqq z,y\coloneqq z,p\coloneqq\mathtt{Refl}_{\sigma}\left(z\right)\right]\\
\Gamma\vdash P:\sigma
\end{array}}{\Gamma\vdash\mathsf{Case}_{\left(p\right):\mathtt{Id}\left(x,y\right).\tau}^{\mathtt{Id}_{\sigma}}\left(z:\sigma.M_{H},P,P,\mathtt{Refl}_{\sigma}\left(P\right)\right)\,\equiv\,M_{H}\left[z\coloneqq P\right]:\tau\left[x\coloneqq P,y\coloneqq P,p\coloneqq\mathtt{Refl}_{\sigma}\left(P\right)\right]}\,\textrm{Id-C}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\mathsf{Case}_{\left(p\right):\mathtt{Id}\left(x,y\right).\tau}^{\mathtt{Id}_{\sigma}}\left(z:\sigma.M_{H},v,v',\mathtt{Refl}_{\sigma}\left(v''\right)\right)\,\rightsquigarrow\,M_{H}\left[z\coloneqq v''\right]}\,\textrm{Id-C-Step}
\]

\end_inset


\end_layout

\begin_layout Standard
this presentation of 
\begin_inset Formula $\textrm{Id-C}$
\end_inset

 is different than the Definitional presentation.
 the standard presentation forces 
\begin_inset Formula $M$
\end_inset

, 
\begin_inset Formula $N$
\end_inset

, and 
\begin_inset Formula $P$
\end_inset

 to be syntactically identical.
 This could allow terms to get stuck in a call by value evaluation because
 of differences in the syntax of typing annotations that are otherwise definitio
nally equivalent.
 This alternative version leaves definitionally equivalence to the type
 system, while not blocking evaluation.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{M\,\rightsquigarrow\,M'}{\mathtt{Refl}_{\sigma}\left(M\right)\,\rightsquigarrow\,\mathtt{Refl}_{\sigma}\left(M'\right)}\,\textrm{Id-I-Step}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{M\,\rightsquigarrow\,M'}{\mathsf{Case}_{\left(p\right):\mathtt{Id}\left(x,y\right).\tau}^{\mathtt{Id}_{\sigma}}\left(z:\sigma.M_{H},M,N,P\right)\rightsquigarrow\mathsf{Case}_{\left(p\right):\mathtt{Id}\left(x,y\right).\tau}^{\mathtt{Id}_{\sigma}}\left(z:\sigma.M_{H},M',N,P\right)}\,\textrm{Id-E-Step-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
