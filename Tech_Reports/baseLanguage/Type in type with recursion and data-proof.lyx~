\begin_body

\begin_layout Title
Type Soundness in an Intensional Dependent Type Theory with Type-in-Type,
 Recursion and Data
\end_layout

\begin_layout Section
Examples
\end_layout

\begin_layout Standard
logical unsoundness:
\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $\mathsf{fun}\,f:\left(x.x\right).\,x:\star.f\,x\qquad:\varPi x:\star.x$
\end_inset


\end_layout

\begin_layout Subsection*
some constructs
\end_layout

\begin_layout Standard
while logically unsound, the language is extremely expressive.
 Conventional data is representable:
\end_layout

\begin_layout Standard
\begin_inset Formula $\mathsf{data}\,Unit.\,\mathsf{where}\,\left\{ tt.\rightarrow Unit\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathsf{data}\,\mathbb{B}.\,\mathsf{where}\,\left\{ true.\rightarrow\mathbb{B}\,|\,false.\rightarrow\mathbb{B}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathsf{data}\,\mathbb{N}.\,\mathsf{where}\,\left\{ z.\rightarrow\mathbb{N}\,|\,suc\,x:\mathbb{N}.\rightarrow\mathbb{N}\right\} $
\end_inset


\end_layout

\begin_layout Standard
data abstraction allows some self-reference
\end_layout

\begin_layout Standard
\begin_inset Formula $0\coloneqq z$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\coloneqq suc\,z$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mathsf{data}\,Vec\,A:\star,n:\mathbb{N}.\,\mathsf{where}\left\{ \begin{array}{cc}
nil\,A:\star. & \rightarrow Vec\,A\,0\\
cons\,A:\star,n:\mathbb{N},-:A,-:Vec\,A\,n. & \rightarrow Vec\,A\,\left(suc\,n\right)
\end{array}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $rep_{\mathbb{B}}\coloneqq\mathsf{fun}\,f:\left(n.Vec\,\mathbb{B}\,n\right).\,n:\mathbb{N}.\mathsf{Case}_{n':\mathbb{N}\rightarrow Vec\,\mathbb{B}\,n'}\,n\,\mathsf{of}\left\{ z\Rightarrow nil\,\mathbb{B}\,|\,suc\,x\Rightarrow cons\,\mathbb{B}\,x\,true\,\left(f\,x\right)\right\} \qquad:\varPi n:\mathbb{N}.Vec\,\mathbb{B}\,n$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Standard
