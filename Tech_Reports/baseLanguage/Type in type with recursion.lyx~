\begin_layout Section
Judgments
\end_layout

\begin_layout Standard
The following judgments are mutually inductively defined.
\end_layout

\begin_layout Subsection
Context Rules
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\lozenge\vdash}\,\textrm{C-Emp}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\star}{\Gamma,x:M\vdash}\,\textrm{C-Ext}
\]

\end_inset


\end_layout

\begin_layout Subsection
Definitional Equality
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash m\Rrightarrow_{\ast}\,n:M\quad\Gamma\vdash m'\Rrightarrow_{\ast}\,n:M}{\Gamma\vdash m\equiv m':M}\,\equiv\textrm{-Def}
\]

\end_inset


\end_layout

\begin_layout Subsection
Conversion
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash m:M\qquad\Gamma\vdash M\equiv M':\star}{\Gamma\vdash m:M'}\,\textrm{Conv}
\]

\end_inset


\end_layout

\begin_layout Subsection
Variables
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:M,\Gamma'\vdash}{\Gamma,x:M,\Gamma'\vdash x:M}\,\textrm{Var}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:M,\Gamma'\vdash}{\Gamma,x:M,\Gamma'\vdash x\Rrightarrow x\,:M}\,\textrm{Var}\textrm{-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Subsection
Type-in-Type
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash}{\Gamma\vdash\star:\star}\,\star\textrm{-F}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash}{\Gamma\vdash\star\Rrightarrow\star\,:\star}\,\star\textrm{-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Subsection
Dependent Recursive Functions
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\star\qquad\Gamma,x:N\vdash\tau:\star}{\Gamma\vdash\Pi x:M.N\,:\star}\,\Pi\textrm{-F}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma,x:M\vdash\tau:\star\qquad\Gamma,f:\Pi x:M.N,x:M\vdash n:N}{\Gamma\vdash\mathsf{fun}\,f.\,x.n\,:\,\varPi x:M.N}\,\Pi\textrm{-I}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash n:\,\Pi x:M.N\qquad\Gamma\vdash m:M}{\Gamma\vdash n\,m\,:\,M\left[x\coloneqq m\right]}\,\Pi\textrm{-E}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\star\qquad\Gamma,x:M\vdash N:\star\qquad\Gamma,f:\Pi x:M.N,x:M\vdash n\Rrightarrow n':N\qquad\Gamma\vdash m\Rrightarrow m':M}{\Gamma\vdash\left(\mathsf{fun}\,f.\,x.n\right)\,m\,\Rrightarrow\,n'\left[x\coloneqq m',f\coloneqq\left(\mathsf{fun}\,f.\,x.n'\right)\right]\,:\,M\left[x\coloneqq m\right]}\,\Pi\textrm{-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Structural Rules
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M\Rrightarrow M'\,:\star\qquad\Gamma,x:M\vdash N\Rrightarrow N'\,:\star}{\Gamma\vdash\Pi x:M.N\,\Rrightarrow\,\Pi x:M'.N'\,:\star}\,\Pi\textrm{-F-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash n\Rrightarrow n'\,:\,\Pi x:M.N\qquad\Gamma\vdash m\Rrightarrow m'\,:M}{\Gamma\vdash n\,m\,\Rrightarrow\,n'\,m'\,:\,N\left[x\coloneqq m\right]}\,\Pi\textrm{-E-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash M:\star\qquad\Gamma,x:M\vdash N:\star\qquad\Gamma,f:\Pi x:M.N,x:M\vdash n\Rrightarrow n':N\qquad\Gamma\vdash m\Rrightarrow m':M}{\Gamma\vdash\left(\mathsf{fun}\,f.\,x.n\right)\,m\,\Rrightarrow\,\left(\mathsf{fun}\,f.\,x.n'\right)\,m'\,:\,M\left[x\coloneqq m\right]}\,\Pi\textrm{-I-\Rrightarrow}
\]

\end_inset


\end_layout

\begin_layout Subsubsection
Call-by-Value
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\,}{\left(\mathsf{fun}\,f.\,x.m\right)\,v\,\rightsquigarrow\,m\left[x\coloneqq v,f\coloneqq\left(\mathsf{fun}\,f.\,x.m\right)\right]}\,\Pi\textrm{-}\rightsquigarrow
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{m\,\rightsquigarrow\,m'}{m\,n\,\rightsquigarrow\,m'\,n}\,\Pi\textrm{-E-\rightsquigarrow-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{n\,\rightsquigarrow\,n'}{v\,n\,\rightsquigarrow\,v\,n'}\,\Pi\textrm{-E-\rightsquigarrow-2}
\]

\end_inset


\end_layout

\begin_layout Subsection
Transitive reflexive closure
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash m:M}{\Gamma\vdash m\Rrightarrow_{\ast}m':M}\,\textrm{\Rrightarrow\ast-refl}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\Gamma\vdash m\Rrightarrow_{\ast}\,m':M\quad\Gamma\vdash m'\Rrightarrow m'':M}{\Gamma\vdash m\Rrightarrow_{\ast}\,m'':M}\,\textrm{\Rrightarrow\ast-step}
\]

\end_inset


\end_layout

